{
  "presets": ["react-app"],
  "plugins": [
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "ssr": true,
          "displayName": true
        }]
      ]
    },
    "production": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "ssr": true,
          "displayName": false
        }]
      ]
    }
  }
}